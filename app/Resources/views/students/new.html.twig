{% extends '_base.html.twig' %}
{% block pageTitle %}new student form{% endblock %}

{% block body %}
    <h1>Create new student</h1>
    {{ form_start(form) }}
    <div class="form-field">
        <div class="errors">
            {{ form_errors(form.name) }}
        </div>
        {{ form_label(form.name) }}
        {{ form_widget(form.name) }}
    </div>
    <div>
        {{ form_row(form.save) }}
    </div>

    {{ form_end(form) }}
{% endblock %}